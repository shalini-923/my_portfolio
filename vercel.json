{
    "version": 2,
    "builds": [
      {
        "src": "Myportfolio/wsgi.py",
        "use": "@vercel/python"
      },
      {
        "src": "vercel-build.sh",
        "use": "@vercel/static-build",
        "config": {
          "outputDirectory": "staticfiles_build/static"
        }
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/static/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/Myportfolio/wsgi.py"
      }
    ],
    "env": {
      "DJANGO_SETTINGS_MODULE": "Myportfolio.settings"
    }
  }
  