{
    "version": 2,
    "builds": [
      {
        "src": "Myportfolio/wsgi.py",  
        "use": "@vercel/python"
      }
    ],
    "routes": [
    {
      "src": "/(.*)",
      "dest": "/Myportfolio/wsgi.py" 
    }
  ],
  "build": {
    "env": {
      "DJANGO_SETTINGS_MODULE": "Myportfolio.settings"
    },
    "commands": [
      "python manage.py collectstatic --noinput" 
    ]
  }
  }
  